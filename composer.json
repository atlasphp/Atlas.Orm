{
    "name": "atlas/atlas",
    "type": "library",
    "description": "ORM: No schema discovery. No migrations. No annotations. No lazy loading. No domain models. No opinions. Just data mapping.",
    "keywords": [ "sql", "data", "mapper", "mapping", "orm" ],
    "homepage": "http://github.com/atlasphp/Atlas.Atlas",
    "license": "MIT",
    "authors": [
        {
            "name": "Atlas Contributors",
            "homepage": "https://github.com/atlasphp/Atlas.Atlas/contributors"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "aura/sql": "~2.0",
        "aura/sqlquery": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Atlas\\": "src/"
        }
    },
    "require-dev": {
        "aura/cli": "~2.0",
        "aura/sqlschema": "~2.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Atlas\\": "tests/"
        }
    }
}
